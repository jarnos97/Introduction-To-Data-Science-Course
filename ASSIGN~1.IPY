    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "#Loading in the dataset\n",
    "data = pd.read_excel(\"slr07.xls\")\n",
    "\n",
    "#Splitting data into seperate variables X and Y\n",
    "X = np.array(data['X'])\n",
    "Y = np.array(data['Y'])\n",
    "\n",
    "#Present a scatterplot displaying the relationship between the two variables\n",
    "plt.scatter(X,Y)\n",
    "plt.title(\"Grey Kangaroos Nose Data\")\n",
    "plt.xlabel(\"Nasal Length\")\n",
    "plt.ylabel(\"Nasal Width\")\n",
    "\n",
    "plt.savefig('scatterplot')\n"
   ]
  },
